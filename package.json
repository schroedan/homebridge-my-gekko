{
  "name": "homebridge-my-gekko",
  "version": "0.2.1",
  "description": "myGEKKO Platform Plugin for Homebridge",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "my-gekko",
    "mygekko"
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "run-s build:clean build:compile",
    "build:clean": "rimraf dist/",
    "build:compile": "tsc",
    "build:compile:prod": "tsc -p tsconfig.prod.json",
    "build:prod": "run-s build:clean build:compile:prod",
    "format": "run-s format:write",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "postpublish": "run-s build:clean",
    "prepublishOnly": "run-s build:prod",
    "start": "hb-service -P . -U .homebridge/ --stdout run",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "author": "Daniel Schr√∂der <daniel.schroeder@skriptfabrik.com>",
  "engines": {
    "homebridge": ">=1.0.0",
    "node": ">=10.17.0"
  },
  "license": "ISC",
  "files": [
    "config.schema.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/schroedan/homebridge-my-gekko.git"
  },
  "bugs": {
    "url": "http://github.com/schroedan/homebridge-my-gekko/issues"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "homebridge": "^1.3.4",
    "homebridge-config-ui-x": "^4.41.1",
    "jest": "^26.6.3",
    "jest-mock-extended": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-organize-imports": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  }
}
