{
  "name": "homebridge-my-gekko",
  "version": "0.2.4",
  "description": "myGEKKO Platform Plugin for Homebridge",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "my-gekko",
    "mygekko"
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "run-s build:clean build:compile",
    "build:clean": "rimraf dist/",
    "build:compile": "tsc",
    "build:compile:prod": "tsc -p tsconfig.prod.json",
    "build:prod": "run-s build:clean build:compile:prod",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postpublish": "run-s build:clean",
    "prepublishOnly": "run-s build:prod",
    "start": "hb-service -P . -U .homebridge/ --stdout run",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "author": "Daniel Schr√∂der <daniel.schroeder@skriptfabrik.com>",
  "engines": {
    "homebridge": ">=1.0.0",
    "node": ">=14.15.0"
  },
  "license": "ISC",
  "files": [
    "config.schema.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/schroedan/homebridge-my-gekko.git"
  },
  "bugs": {
    "url": "http://github.com/schroedan/homebridge-my-gekko/issues"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^14.18.26",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "homebridge": "^1.5.0",
    "homebridge-config-ui-x": "^4.50.1",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "packageManager": "yarn@3.2.3"
}
